datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model RecentlyPlayedSong {
  id        String   @id @default(uuid())
  title     String
  artist    String
  album     String?
  playedAt  DateTime @default(now())
}

model MadeForYouAlbum {
  id        String   @id @default(uuid())
  title     String
  artist    String
  albumArt  String
  createdAt DateTime @default(now())
}

model PopularAlbum {
  id        String   @id @default(uuid())
  title     String
  artist    String
  albumArt  String
  createdAt DateTime @default(now())
}